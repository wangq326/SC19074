\name{rrr2}
\alias{rrr2}
\title{RRR model.}
\usage{
rrr2(
  Y,
  X,
  penaltySVD = c("rank", "ann"),
  ic.type = c("GIC", "AIC", "BIC", "BICP", "GCV"),
  df.type = c("exact", "naive"),
  maxrank = min(dim(Y), dim(X)),
  modstr = list(),
  control = list()
)
}
\arguments{
\item{Y}{the resopnce matrix}

\item{X}{the design matrix}

\item{penaltySVD}{the penalty term}

\item{ic.type}{"GIC", "AIC", "BIC", "BICP" or "GCV"}

\item{df.type}{"exact" or "naive")}

\item{maxrank}{the max rank to be expected}

\item{modstr}{the mod}

\item{control}{the control of SVD and QR}
}
\value{
the solution of rrr.
}
\description{
The model is developed by Kun Chen.
}
\examples{
\dontrun{
null
}
}
